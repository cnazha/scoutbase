    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.3",
    "pg": "^7.12.1"
  }
